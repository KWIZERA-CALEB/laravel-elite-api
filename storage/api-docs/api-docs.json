{
    "openapi": "3.0.0",
    "info": {
        "title": "App",
        "version": "1.0.0"
    },
    "paths": {
        "/api/register": {
            "post": {
                "description": "Register User",
                "operationId": "ca7c0cb6772de9bffbb8c896980cebc8",
                "responses": {
                    "201": {
                        "description": "User registered",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "api/login": {
            "post": {
                "description": "Login User",
                "operationId": "7128d88221be30b754428a5db2880cd0",
                "responses": {
                    "200": {
                        "description": "User loggined",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "User": {
                "required": [
                    "name",
                    "email",
                    "password",
                    "profile",
                    "role",
                    "age"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "format": "password"
                    },
                    "profile": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    },
                    "age": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    }
}